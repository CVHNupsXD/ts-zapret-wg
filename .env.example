# Tailscale auth key (generate at https://login.tailscale.com/admin/settings/keys)
TS_AUTHKEY=tskey-auth-xxxxx

# AdGuard Home API credentials
AGH_URL=http://127.0.0.1:3000
AGH_USER=admin
AGH_PASS=your_password

# Domain resolution interval in seconds (default: 43200 = 12 hours)
RESOLVE_INTERVAL=43200